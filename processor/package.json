{
  "name": "kitspace-processor",
  "version": "0.0.1",
  "description": "Asset processor for kitspace-v2",
  "main": "index.js",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "dev": "nodemon src/server.ts --watch src/",
    "start": "ts-node src/server.ts",
    "test": "LOG_LEVEL=debug DATA_DIR=/data/test mocha test/integration/**/*.ts -r ts-node/register --timeout=120000 --exit",
    "posttest": "rm -rf /data/test",
    "tsc": "tsc",
    "lint": "eslint src/ test/ --ext ts",
    "fmt": "prettier '*.json' src/ test/ --write"
  },
  "dependencies": {
    "1-click-bom": "https://github.com/kitspace/npm-1-click-bom#master",
    "async-lock": "^1.3.2",
    "backoff": "^2.5.0",
    "bullmq": "^1.66.0",
    "cheerio": "^1.0.0-rc.10",
    "chokidar": "^3.4.3",
    "escape-html": "^1.0.3",
    "express": "^4.17.2",
    "express-fileupload": "^1.2.1",
    "globule": "^1.0.0",
    "js-yaml": "^3.3.1",
    "jszip": "^3.7.0",
    "linkify-it": "^4.0.1",
    "lodash.debounce": "^4.0.8",
    "loglevel": "^1.7.1",
    "meilisearch": "^0.25.1",
    "pcb-stackup": "^4.2.5",
    "postgres": "^3.2.2",
    "prexit": "^1.0.0",
    "ramda": "^0.27.1",
    "shell-escape": "^0.2.0",
    "superagent": "^6.1.0",
    "ts-node": "^10.5.0",
    "url-slug": "^3.0.4",
    "whats-that-gerber": "^4.2.2",
    "xml-element-string": "^1.0.0"
  },
  "devDependencies": {
    "@types/async-lock": "^1.1.5",
    "@types/escape-html": "^1.0.2",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.2",
    "@types/globule": "^1.1.4",
    "@types/linkify-it": "^3.0.2",
    "@types/lodash.debounce": "^4.0.6",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.17",
    "@types/shell-escape": "^0.2.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "typescript": "^4.5.5"
  }
}

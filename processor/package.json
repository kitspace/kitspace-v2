{
  "name": "kitspace-processor",
  "version": "0.0.1",
  "description": "Asset processor for kitspace-v2",
  "license": "AGPL-3.0",
  "private": true,
  "type": "module",
  "exports": "./index.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "tsc-watch --noClear --onSuccess 'bash -c \"yarn cp-assets && node ./dist/src/server.js\"'",
    "cp-assets": "find ./src ! -name '*.ts' -exec cp -r '{}' ./dist/src/ \\;",
    "cp-test-assets": "find ./test ! -name '*.ts' -exec cp -r '{}' ./dist/test/ \\;",
    "test": "mocha --require source-map-support/register dist/test/integration/**/*.js --timeout=120000 --exit",
    "posttest": "rm -rf /data/test /app/dist",
    "lint": "eslint src/ test/ --ext ts",
    "prettier": "prettier '*.json' src/ test/",
    "fmt": "yarn prettier --write"
  },
  "dependencies": {
    "1-click-bom": "https://github.com/kitspace/npm-1-click-bom#master",
    "async-lock": "^1.3.2",
    "bullmq": "^1.91.1",
    "cheerio": "^1.0.0-rc.10",
    "chokidar": "^3.4.3",
    "express": "^4.17.2",
    "express-fileupload": "^1.2.1",
    "globule": "^1.0.0",
    "hast-util-has-property": "^2.0.0",
    "js-yaml": "^4.1.0",
    "jszip": "^3.7.0",
    "lodash.debounce": "^4.0.8",
    "loglevel": "^1.7.1",
    "meilisearch": "^0.25.1",
    "pcb-stackup": "^4.2.5",
    "postgres": "^3.2.2",
    "prexit": "^1.0.0",
    "ramda": "^0.27.1",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-highlight": "^5.0.2",
    "rehype-raw": "^6.1.1",
    "rehype-sanitize": "^5.0.1",
    "rehype-shift-heading": "^1.0.2",
    "rehype-slug": "^5.0.1",
    "rehype-stringify": "^9.0.3",
    "remark-emoji": "^3.0.2",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "shell-escape": "^0.2.0",
    "superagent": "^6.1.0",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.1",
    "whats-that-gerber": "^4.2.2",
    "xml-element-string": "^1.0.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/async-lock": "^1.1.5",
    "@types/escape-html": "^1.0.2",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.2",
    "@types/globule": "^1.1.4",
    "@types/js-yaml": "^4.0.5",
    "@types/linkify-it": "^3.0.2",
    "@types/lodash.debounce": "^4.0.6",
    "@types/marked": "^4.0.7",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.17",
    "@types/ramda": "^0.28.15",
    "@types/shell-escape": "^0.2.1",
    "@types/superagent": "^4.1.15",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "canvas": "^2.10.1",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^10.0.0",
    "prettier": "^2.5.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.2",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.5.5"
  }
}

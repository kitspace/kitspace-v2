{
  "name": "kitspace-processor",
  "version": "0.0.1",
  "description": "Asset processor for kitspace-v2",
  "main": "index.js",
  "license": "AGPL-3.0",
  "scripts": {
    "dev": "nodemon src/server.js --watch src/",
    "start": "node src/server.js",
    "test": "LOG_LEVEL=debug DATA_DIR=/data/test mocha test/integration --timeout=120000",
    "posttest": "rm -rf /data/test",
    "fmt": "prettier --write '*.json' $(find src/ test/ -name '*.js')"
  },
  "dependencies": {
    "1-click-bom": "https://github.com/kitspace/npm-1-click-bom#78df7bb0ebfbdb7beee99d13514b3737f010508f",
    "cheerio": "^1.0.0-rc.10",
    "chokidar": "^3.4.3",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "globule": "^1.0.0",
    "js-yaml": "^3.3.1",
    "jszip": "^3.7.0",
    "lodash.debounce": "^4.0.8",
    "loglevel": "^1.7.1",
    "pcb-stackup": "^4.2.5",
    "ramda": "^0.27.1",
    "superagent": "^6.1.0",
    "whats-that-gerber": "^4.2.2",
    "xml-element-string": "^1.0.0"
  },
  "devDependencies": {
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "supertest": "^6.0.1"
  }
}
